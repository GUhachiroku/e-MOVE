<% if rooms.any? %>
  <ul class="list-unstyled">
    <% rooms.each do |room| %>
      <li class="media">

        <div class="media-body">
         
          <div>
            <p><%= link_to room.name, room_path(room) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>

<% end %>